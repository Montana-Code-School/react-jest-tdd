{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "parser": "babel-eslint",

  "rules": {
    "no-var": 0,
    "no-else-return": 0,
    "func-names": 0,
    "new-cap": 1,
    "vars-on-top": 0,
    "padded-blocks": 0,
    "comma-dangle": 0,
    "no-unused-vars": 0,
    "no-alert": 0
  },
  "plugins": [
    "react"
  ],
  "extends": "airbnb"
}







